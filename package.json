{
  "name": "node-static-server",
  "version": "1.0.0",
  "description": "Static HTML server using Node.js",
  "main": "index.js",
  "scripts": {
    "www": "http-server ./www -p 8080 -o",
    "api": "node src/server.js",
    "all": "concurrently -n api,web -c blue,green \"npm run api\" \"npm run www\""
  },
  "keywords": [
    "static",
    "html",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "http-server": "^14.1.1",
    "uuid": "^13.0.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5"
  }
}
